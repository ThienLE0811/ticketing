    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-pdf-viewer/core": "^3.6.0",
    "@react-pdf-viewer/default-layout": "^3.6.0",
    "@react-pdf-viewer/locales": "^1.0.0",
    "@tinymce/tinymce-react": "^4.1.0",
    "@types/ckeditor__ckeditor5-core": "^33.0.3",
    "@umijs/route-utils": "^2.1.3",
    "ahooks": "^3.5.2",
    "antd": "^4.20.0",
    "classnames": "^2.3.0",
    "dompurify": "^2.3.10",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "i": "^0.3.7",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "npm": "^8.15.0",
    "omit.js": "^2.0.2",
    "pdfjs-dist": "^2.14.305",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@playwright/test": "^1.17.0",
    "@types/ckeditor__ckeditor5-build-classic": "^29.0.1",
    "@types/ckeditor__ckeditor5-build-decoupled-document": "^29.1.1",
    "@types/ckeditor__ckeditor5-html-embed": "^31.0.0",
    "@types/classnames": "^2.3.1",
    "@types/cookie": "^0.5.1",
    "@types/dompurify": "^2.3.3",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.11.1",
    "@umijs/max": "^4.0.11",
    "@umijs/openapi": "^1.3.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.0",
    "swagger-ui-dist": "^4.12.0",
    "typescript": "^4.5.0",
    "umi-presets-pro": "^1.0.1",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "resolutions": {
    "react-devtools-core": "4.14.0"
  }
}